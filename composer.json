{
  "name": "alexmazorchuk/sorter-test",
  "description": "This is a test sorter library for array, integer or rows in PHP .",
  "keywords": ["test", "sort", "array", "integer", "string"],
  "type": "library",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Mazorchuk",
      "email": "019maz@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "Alexmazorchuk\\Sort\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alexmazorchuk\\Sort\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --diff --ansi",
    "tests": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
